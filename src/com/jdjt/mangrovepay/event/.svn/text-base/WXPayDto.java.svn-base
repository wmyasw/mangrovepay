package com.jdjt.mangrovepay.event;

public class WXPayDto {
	// 订单编号列表 多个的场合，中间用“,”分割。
	String orderNoList;
	
	String orderIdList;
	// 账户支付
	AccountPayDto accountInfo;
	// 储值卡支付
	CardPayDto epayCardInfo;
	// 在线支付
	OlinePayDto onlineInfo;

	
	public String getOrderIdList() {
		return orderIdList;
	}

	public void setOrderIdList(String orderIdList) {
		this.orderIdList = orderIdList;
	}

	public String getOrderNoList() {
		return orderNoList;
	}

	public void setOrderNoList(String orderNoList) {
		this.orderNoList = orderNoList;
	}

	public AccountPayDto getAccountInfo() {
		return accountInfo;
	}

	public void setAccountInfo(AccountPayDto accountInfo) {
		this.accountInfo = accountInfo;
	}

	public CardPayDto getEpayCardInfo() {
		return epayCardInfo;
	}

	public void setEpayCardInfo(CardPayDto epayCardInfo) {
		this.epayCardInfo = epayCardInfo;
	}

	public OlinePayDto getOnlineInfo() {
		return onlineInfo;
	}

	public void setOnlineInfo(OlinePayDto onlineInfo) {
		this.onlineInfo = onlineInfo;
	}

}
